CompileFlags:
  Add: [-Iinclude, -Isrc]

Completion:
  HeaderInsertion: IWYU
  AllScopes: Yes
  ArgumentLists: FullPlaceholders
  CodePatterns: All

InlayHints:
  BlockEnd: true
  Designators: true
  Enabled: true
  ParameterNames: true
  DeducedTypes: true
  DefaultArguments: true
  TypeNameLimit: 24

Diagnostics:
  UnusedIncludes: None

Index:
  StandardLibrary: false
